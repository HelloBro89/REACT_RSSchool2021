{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb/base",
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "overrides": [
        {
            "files": [
                "*.jsx",
                "*.js"
            ]
        }
    ],
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "no-param-reassign": "off",
        "react/prop-types": "off",
        "import/prefer-default-export": "off",
        "no-console": "off"
        // "react/react-in-jsx-scope": "off",
        // "no-use-before-define": "off"
        // "import/prefer-default-export": "off"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}